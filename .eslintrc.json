{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":["error", "windows"],
        "semi":"off",
        "indent": ["error", 4],
        "object-curly-spacing": ["error", "always"],
        "max-len": ["error", { "code": 127 }]
    }
}
